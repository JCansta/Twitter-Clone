<div class="container">
    <div class="row justify-content-center">
        <div class="col-10">
            <%= form_for :create, :multipart => true, class: "shadow p-3 mb-3 bg-info rounded" do |f| %>
                Select an Excel File :
                <%= f.file_field :file%>
                <%= f.submit 'Submit', data: { disable_with: "Submitting..." }, class: "btn btn-outline-light btn-lg" %>
            <% end %>
        </div>
    </div>
</div>